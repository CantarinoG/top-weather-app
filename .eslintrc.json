{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "eol-last": "off",
        "no-restricted-globals": "off",
        "eqeqeq" : "off",
        "no-alert":"off",
        "import/prefer-default-export": "off"
    }
}